logPrint "Processing ~/.bashrc.$(hostname -s)"

CUSTOM_NODE_CONFIG_FILE=TRUE

### TQ-TODO: Do we need this in .bashrc.init as well?
export POD_BASE="/tmp/none"
export CHANGELOG_DST=taisto@ip-s.se
export NSD_INT=/var/nsd/zones/int/
export NSD_EXT=/var/nsd/zones/ext/
export TERM=xterm-256color
export SSH_AGENT_PID="DISABLE"
export PY3=/root/py3

alias egrep="\ggrep -E"
alias grep='\ggrep -E -i'
alias psg="pgrep -fl"
alias wg-status='ifconfig wg | egrep "wg[01]|wgpeer|wgendp|handsh|tx:"'
